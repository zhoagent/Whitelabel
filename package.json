{
  "name": "nkzw-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "dev": "expo start",
    "dev:setup": "pnpm fbtee",
    "fbtee": "pnpm run fbtee:manifest && pnpm run fbtee:collect && pnpm run fbtee:translate",
    "fbtee:collect": "fbtee collect --manifest < .src_manifest.json > .source_strings.json",
    "fbtee:manifest": "fbtee manifest --src src",
    "fbtee:translate": "fbtee translate --translations translations/*.json --jenkins --output-dir src/translations/",
    "format": "prettier --write .",
    "format-graphql": "./scripts/format-graphql-schema.tsx",
    "preinstall": "command -v git >/dev/null 2>&1 && git config core.hooksPath git-hooks || exit 0",
    "ios": "expo run:ios --device 'iPhone 16 Pro'",
    "lint": "eslint --cache .",
    "lint:format": "prettier --cache --check .",
    "prebuild": "expo prebuild",
    "start": "expo start --dev-client",
    "test": "NODE_OPTIONS='--no-experimental-detect-module' pnpm exec npm-run-all2 --parallel tsc:check vitest:run lint lint:format",
    "tsc:check": "tsc",
    "vitest:run": "vitest run",
    "web": "expo start --web"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.1.4",
    "@legendapp/list": "^1.0.11",
    "@nkzw/core": "^1.2.1",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/stack": "^7.3.2",
    "@supabase/supabase-js": "^2.43.4",
    "@tanstack/react-query": "^5.40.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "babel-plugin-react-compiler": "19.1.0-rc.1",
    "classnames": "^2.5.1",
    "expo": "~53.0.9",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-font": "~13.3.1",
    "expo-linear-gradient": "^14.1.4",
    "expo-linking": "~7.1.5",
    "expo-localization": "^16.1.5",
    "expo-notifications": "~0.28.9",
    "expo-router": "~5.0.6",
    "expo-secure-store": "~13.0.2",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "fbtee": "^0.2.2",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "experimental",
    "react-native": "0.79.2",
    "react-native-edge-to-edge": "^1.6.0",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-purchases": "^7.10.0",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "^15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.20.0",
    "zustand": "^4.5.2",
    "expo-status-bar": "~2.2.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@nkzw/babel-preset-fbtee": "^0.2.2",
    "@nkzw/eslint-config": "^2.4.0",
    "@nkzw/eslint-plugin-fbtee": "^0.2.2",
    "@react-native/metro-babel-transformer": "^0.79.2",
    "@types/react": "~18.2.79",
    "@vitejs/plugin-react": "^4.4.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.26.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "expo-build-disk-cache": "^0.2.0",
    "npm-run-all2": "^8.0.1",
    "prettier": "4.0.0-alpha.12",
    "prettier-plugin-packagejson": "^2.5.12",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-native-svg-transformer": "^1.5.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "vitest-react-native": "^0.1.5",
    "@babel/core": "^7.20.0"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "tailwindcss"
      ]
    },
    "patchedDependencies": {
      "metro": "patches/metro.patch"
    },
    "ignorePatchFailures": false
  }
}
